<Window x:Class="fsd123_project5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fsd123_project5"
        mc:Ignorable="d"
        Title="Sanwich Maker" Height="200" Width="400">
    <StackPanel>
        <Button Name="btnSandwichSelector" Content="Make a sandwich" HorizontalAlignment="Center" Margin="10" Width="361" Click="btnSandwichSelector_Click"/>
        <DockPanel>
            <Label Content="Bread" Width="200"></Label>
            <Label Name="lblBread" Content="..."></Label>
        </DockPanel>
        <DockPanel>
            <Label Content="Veggies" Width="200"></Label>
            <Label Name="lblVeggies" Content="..."></Label>
        </DockPanel>
        <DockPanel>
            <Label Content="Meat" Width="200"></Label>
            <Label Name="lblMeat" Content="..."></Label>
        </DockPanel>
       
    </StackPanel>
</Window>
